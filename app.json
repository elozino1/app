{
  "expo": {
    "name": "merchant_dashboard_mobile_app",
    "slug": "merchant_dashboard_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/seerbit_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF0000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splash_screen.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
